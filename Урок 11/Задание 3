numbers = 4
ip_num = []
for i in range(numbers):
    number = input('Введите число: ')
    ip_num.append(number)
ip_arr = '.'.join(ip_num)
value_ip = ip_arr.split('.')
if len(value_ip[0]) == 3 and len(value_ip[1]) == 3 and len(value_ip[2]) == 1 and len(value_ip[3]) == 1:
    print('yes')
else:
    print('no')
